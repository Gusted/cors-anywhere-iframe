{
    "compilerOptions": {
        "target": "es2019",
        "baseUrl": ".",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": [
            "es2015",
            "es2017",
            "es2019"
        ],
        "downlevelIteration": true,
        "noEmit": true,
        "incremental": true,
        "esModuleInterop": true,
        "isolatedModules": true
    },
    "exclude": [
        "node_modules"
    ]
}
